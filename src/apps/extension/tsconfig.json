{
  "extends": "./.wxt/tsconfig.json",
  "compilerOptions": {

    "jsx": "preserve",

    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["./src/**/*"],
  "exclude": [
    "node_modules",
    "**/*.native.tsx",
    "**/*.native.ts",
    "**/*.native.jsx",
    "**/*.native.js"
  ]
}