{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "build": {
      "outputs": ["dist/**", "build/**", "public/**"]
    },
    "start": {
      "cache": false,
      "persistent": true
    }
  }
}
